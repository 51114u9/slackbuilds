<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-nmea">
    <sub-class-of type="text/plain"/>
    <comment xml:lang="en">NMEA data</comment>
    <comment xml:lang="de">NMEA-Daten</comment>
    <comment xml:lang="es">datos NMEA</comment>
    <glob pattern="*.nmea" />
    <glob pattern="*.nma" />
    <glob pattern="*.nme" />
  </mime-type>
  <mime-type type="application/x-geojson">
    <sub-class-of type="text/plain"/>
    <comment xml:lang="en">GeoJSON data</comment>
    <comment xml:lang="de">GeoJSON-Daten</comment>
    <comment xml:lang="es">datos GeoJSON</comment>
    <glob pattern="*.geojson" />
    <glob pattern="*.json" />
  </mime-type>
  <mime-type type="application/x-osm+xml">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">OpenStreetMap data</comment>
    <comment xml:lang="de">OpenStreetMap-Daten</comment>
    <comment xml:lang="es">datos OpenStreetMap</comment>
    <magic priority="80">
      <match value="&lt;osm" type="string" offset="0:512"/>
    </magic>
    <glob pattern="*.osm" />
    <glob pattern="*.osc" />
  </mime-type>
  <mime-type type="application/x-gzip-compressed-osm+xml">
    <sub-class-of type="application/x-gzip"/>
    <comment xml:lang="en">OpenStreetMap data</comment>
    <comment xml:lang="de">OpenStreetMap-Daten</comment>
    <comment xml:lang="es">datos OpenStreetMap</comment>
    <glob pattern="*.osm.gz" />
    <glob pattern="*.osc.gz" />
  </mime-type>
  <mime-type type="application/x-bzip-compressed-osm+xml">
    <sub-class-of type="application/x-bzip"/>
    <comment xml:lang="en">OpenStreetMap data</comment>
    <comment xml:lang="de">OpenStreetMap-Daten</comment>
    <comment xml:lang="es">datos OpenStreetMap</comment>
    <glob pattern="*.osm.bz2" />
    <glob pattern="*.osc.bz2" />
  </mime-type>
  <mime-type type="application/x-gpx+xml">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">GPX track</comment>
    <comment xml:lang="de">GPX-Track</comment>
    <comment xml:lang="es">traza GPX</comment>
    <magic priority="90">
      <match value="&lt;gpx" type="string" offset="0:256"/>
    </magic>
    <glob pattern="*.gpx" />
    <glob pattern="*.GPX" />
    <root-XML namespaceURI="http://www.topografix.com/GPX/1/0" localName="gpx"/>
  </mime-type>
  <mime-type type="application/x-gzip-compressed-gpx+xml">
    <sub-class-of type="application/x-gzip"/>
    <comment xml:lang="en">GPX track</comment>
    <comment xml:lang="de">GPX-Track</comment>
    <comment xml:lang="es">traza GPX</comment>
    <glob pattern="*.gpx.gz" />
    <glob pattern="*.GPX.gz" />
  </mime-type>
  <mime-type type="application/x-bzip-compressed-gpx+xml">
    <sub-class-of type="application/x-bzip"/>
    <comment xml:lang="en">GPX track</comment>
    <comment xml:lang="de">GPX-Track</comment>
    <comment xml:lang="es">traza GPX</comment>
    <glob pattern="*.gpx.bz2" />
    <glob pattern="*.GPX.bz2" />
  </mime-type>
  <mime-type type="application/x-kml+xml">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">KML track</comment>
    <comment xml:lang="de">KML-Track</comment>
    <comment xml:lang="es">traza KML</comment>
    <magic priority="90">
      <match value="&lt;kml" type="string" offset="0:256"/>
    </magic>
    <glob pattern="*.kml" />
    <glob pattern="*.KML" />
    <root-XML namespaceURI="http://www.opengis.net/kml/2.2" localName="kml"/>
  </mime-type>
  <mime-type type="application/x-gzip-compressed-kml+xml">
    <sub-class-of type="application/x-gzip"/>
    <comment xml:lang="en">KML track</comment>
    <comment xml:lang="de">KML-Track</comment>
    <comment xml:lang="es">traza KML</comment>
    <glob pattern="*.kml.gz" />
    <glob pattern="*.KML.gz" />
  </mime-type>
  <mime-type type="application/x-bzip-compressed-kml+xml">
    <sub-class-of type="application/x-bzip"/>
    <comment xml:lang="en">KML track</comment>
    <comment xml:lang="de">KML-Track</comment>
    <comment xml:lang="es">traza KML</comment>
    <glob pattern="*.kml.bz2" />
    <glob pattern="*.KML.bz2" />
  </mime-type>
  <mime-type type="application/x-josm-preset">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">JOSM preset</comment>
    <comment xml:lang="de">JOSM-Objektvorlage</comment>
    <comment xml:lang="es">predeterminado JOSM</comment>
    <root-XML namespaceURI="http://josm.openstreetmap.de/tagging-preset-1.0" localName="presets"/>
    <magic priority="80">
      <match value="&lt;presets" type="string" offset="0:4096"/>
      <match value="&lt;annotations" type="string" offset="0:4096"/>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
  </mime-type>
  <mime-type type="application/x-josm-style">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">JOSM mappaint style</comment>
    <comment xml:lang="de">JOSM-Zeichenvorlage</comment>
    <comment xml:lang="es">estilo pintado JOSM</comment>
    <root-XML namespaceURI="http://josm.openstreetmap.de/mappaint-style-1.0" localName="rules"/>
    <magic priority="80">
      <match value="&lt;rules" type="string" offset="0:4096"/>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
  </mime-type>
  <mime-type type="application/x-josm-maps">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">JOSM map sources</comment>
    <comment xml:lang="de">JOSM-Kartenquellen</comment>
    <comment xml:lang="es">fuentes mapa JOSM</comment>
    <root-XML namespaceURI="http://josm.openstreetmap.de/maps-1.0" localName="imagery"/>
    <magic priority="80">
      <match value="&lt;imagery" type="string" offset="0:4096"/>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
  </mime-type>
  <mime-type type="application/x-josm-preferences">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">JOSM preferences</comment>
    <comment xml:lang="de">JOSM-Einstellungen</comment>
    <comment xml:lang="en">preferencias JOSM</comment>
    <root-XML namespaceURI="http://josm.openstreetmap.de/preferences-1.0" localName="preferences"/>
    <magic priority="80">
      <match value="&lt;preferences" type="string" offset="0:4096"/>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
  </mime-type>
  <mime-type type="text/x-mapcss">
    <sub-class-of type="text/css"/>
    <comment xml:lang="en">MapCSS paint style</comment>
    <comment xml:lang="de">MapCSS-Zeichenvorlage</comment>
    <comment xml:lang="en">estilo pintado MapCSS</comment>
    <glob pattern="*.mapcss" />
  </mime-type>
</mime-info>
